{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "declaration": true,
        "jsx": "react-jsx",
        "lib": ["DOM", "ES2015", "ES2015.promise", "ES2015.Proxy", "ES2017"],
        "strict": true,
        "skipLibCheck": false,
        "esModuleInterop": true
    },
    "include": ["src/**/*"]
}
